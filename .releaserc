{
    "branches": ["main"],
    "plugins": [
        "@semantic-release/changelog",
        {
            "path": "@semantic-release/github",
            "assets": [
                {
                    "path": "my-go-binary",
                    "label": "Go Binary for version ${nextRelease.version}"
                }
            ]
        },
        [
            "@semantic-release/git",
            {
                "assets": ["CHANGELOG.md"],
                "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
            }
        ]
    ]
}
